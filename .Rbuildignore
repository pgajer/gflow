# pkgdown site and docs
^_pkgdown\.yml$
^docs$
^pkgdown$

# RStudio project files
^.*\.Rproj$
^\.Rproj\.user$

# Git and GitHub files
^\.git$
^\.gitignore$
^\.github$

# CRAN submission / comments / metadata
^README\.md$
^NEWS\.md$
^cran-comments\.md$
^CRAN-SUBMISSION\.md$

# Local build / check scripts and logs
^gflow_check_results\.txt$
^build_and_check\.sh$
^check_cran\.sh$
^check_dev\.R$
^Makefile$
^gflow-manual\.pdf$

# Build artefacts
^gflow\.Rcheck$
^gflow_.*\.tar\.gz$

# OS cruft
^\.DS_Store$
^\.Rapp\.history$

# Temporary / local directories
^\.claude$
^CLAUDE\.md$

# Object and shared libs
^src/.*\.(o|so|dll|a)$

# Vignettes (exclude sources if not shipping)
^[Vv]ignettes$
^inst/doc$
